{
  "name": "twitch-overlay",
  "version": "1.1.0",
  "description": "Overlay for Twitch streamers that can be loaded into OBS or XSplit",
  "keywords": [
    "twitch",
    "obs",
    "xsplit"
  ],
  "main": "index.js",
  "scripts": {
    "start": "node index.js",
    "build": "./node_modules/.bin/webpack",
    "dev": "./node_modules/.bin/webpack -d -w",
    "test": "mocha lib/**/*.spec.js lib/**/**/*.spec.js models/**/*.spec.js --exit",
    "watch": "mocha -w lib/**/*.spec.js"
  },
  "engines": {
    "node": ">=4.4.0"
  },
  "author": "bdickason",
  "contributors": [
    {
      "name": "bdickason",
      "email": "dickason@gmail.com"
    }
  ],
  "repository": {
    "type": "git",
    "url": "git://github.com/bdickason/twitch-overlay.git"
  },
  "dependencies": {
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.2",
    "babel-preset-env": "^1.6.1",
    "babel-preset-react": "^6.24.1",
    "express": "4.15.2",
    "extend": "3.0.0",
    "pug": "2.0.0-beta11",
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "react-html-parser": "^2.0.2",
    "shortid": "^2.2.8",
    "socket.io": "1.7.3",
    "twitch-overlay-html": "1.1.0",
    "twitch-overlay-text": "1.1.0",
    "twitch-overlay-video": "1.1.0",
    "webpack": "^3.8.1"
  },
  "license": "GPL-3.0",
  "devDependencies": {
    "chai": "^4.1.2",
    "css-loader": "^0.28.7",
    "mocha": "^4.0.1",
    "sinon": "^4.1.3",
    "style-loader": "^0.19.0"
  }
}
